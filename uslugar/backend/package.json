{
  "name": "uslugar-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "NODE_ENV=development node src/server.js",
    "prisma:generate": "prisma generate --schema=prisma/schema.prisma",
